   "source": [
    "import re\n",
    "raw_file=\"name.md\"  #输入需转换的文件名\n",
    "made_file=\"name2.md\"     #给转换后的文件命名\n",
    "with open(raw_file,\"r\",encoding=\"utf-8\") as f:  #utf-8或gbk\n",
    "    s = f.read()\n",
    "t = re.sub(r\"\\!\\[(.*)\\]\\(.*?\\)\\1\",r\"$\\1$\",s)\n",
    "with open(made_file,\"w\",encoding='utf-8') as f:  #encoding='utf-8'\n",
    "    f.write(t)\n"
   ]
